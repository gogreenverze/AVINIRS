<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="400" height="200" viewBox="0 0 400 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Border Rectangle with Gaps -->
  <!-- Left side -->
  <path d="M50,30 L70,30" stroke="#aaaaaa" stroke-width="2" fill="none" />
  <path d="M50,30 L50,170" stroke="#aaaaaa" stroke-width="2" fill="none" />

  <!-- Top side with gaps for AVINI LABS -->
  <path d="M90,30 L120,30" stroke="#aaaaaa" stroke-width="2" fill="none" />
  <path d="M140,30 L160,30" stroke="#aaaaaa" stroke-width="2" fill="none" />
  <path d="M180,30 L220,30" stroke="#aaaaaa" stroke-width="2" fill="none" />
  <path d="M240,30 L260,30" stroke="#aaaaaa" stroke-width="2" fill="none" />
  <path d="M280,30 L350,30" stroke="#aaaaaa" stroke-width="2" fill="none" />

  <!-- Right side -->
  <path d="M350,30 L350,170" stroke="#aaaaaa" stroke-width="2" fill="none" />

  <!-- Bottom side with gap for ECG line -->
  <path d="M50,170 L200,170" stroke="#aaaaaa" stroke-width="2" fill="none" />
  <path d="M300,170 L350,170" stroke="#aaaaaa" stroke-width="2" fill="none" />

  <!-- Main Logo Text -->
  <text x="200" y="100" font-family="Arial, sans-serif" font-size="48" font-weight="700" text-anchor="middle" fill="#d4006e">AVINI LABS</text>

  <!-- Tagline Text -->
  <text x="200" y="130" font-family="Arial, sans-serif" font-size="14" font-weight="400" text-anchor="middle" fill="#888888" letter-spacing="4">ACCURATE - RELIABLE - ADVANCED</text>

  <!-- Enhanced ECG Line and Heart -->
  <g transform="translate(200, 170)">
    <!-- ECG Line Left -->
    <path d="M-50,0 L-30,0 L-25,-10 L-20,10 L-15,0 L-10,0" stroke="#d4006e" stroke-width="2" fill="none" />

    <!-- ECG Line Middle -->
    <path d="M-10,0 L0,0" stroke="#d4006e" stroke-width="2" fill="none" />

    <!-- Heart Section -->
    <path d="M0,0 C5,-15 20,-15 25,0 C30,-15 45,-15 50,0" stroke="#d4006e" stroke-width="2" fill="none" />

    <!-- Heart Fill -->
    <path d="M25,0 C20,-15 5,-15 0,0 C5,15 20,15 25,0 C30,15 45,15 50,0 C45,-15 30,-15 25,0 Z" fill="#d4006e" />

    <!-- ECG Line Right -->
    <path d="M50,0 L60,0 L65,-5 L70,5 L75,0 L100,0" stroke="#d4006e" stroke-width="2" fill="none" />
  </g>
</svg>
